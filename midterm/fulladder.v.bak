module fulladder(input A, B, Cin, output Cout, S)
assign S = A^B^Cin;
assign Cout = (A&B)|(Cin&(A^B));
endmodule

module carryadder(input wire [3:0] A, B, input wire [4:0] C, output wire [3:0] S))

fulladder num1(A[0], B[0], (0), C[0], S[0]);
fulladder num2(A[1], B[1], C[0], C[1], S[1]);
fulladder num2(A[1], B[1], C[1], C[2], S[1]);
fulladder num2(A[3], B[3], C[2], C[3], S[1]);

endmodule
